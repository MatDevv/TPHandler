local colleseem = script.Parent:FindFirstChild("Colleseum")
local gyro = script.Parent:FindFirstChild("Gyro")
local JotaroTP = script.Parent:FindFirstChild("JotaroTP")
local KilluaTP = script.Parent:FindFirstChild("KilluaTP")
local PucciTP= script.Parent:FindFirstChild("PucciTP")
local QuestNPCTP = script.Parent:FindFirstChild("QuestNPCTP")
local SakuyaTP = script.Parent:FindFirstChild("SakuyaTP")
local ShopTP = script.Parent:FindFirstChild("ShopTP")
local UmbraTP = script.Parent:FindFirstChild("UmbraTP")



gyro.MouseButton1Down:Connect(function()

	local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
	local location = game:GetService("Workspace").NPCS.Gyro["head 2"].CFrame
	local humanoid = game.Players.LocalPlayer.Character.Humanoid
	humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	wait(0.1)
	pl.CFrame = location

end)


colleseem.MouseButton1Down:Connect(function()

	local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
	local location = game:GetService("Workspace").Map.Layout.Collisions.Colosseum.MeshPart.CFrame
	local humanoid = game.Players.LocalPlayer.Character.Humanoid
	humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	wait(0.1)
	pl.CFrame = location

end)


